{
  "name": "cdb-graphql",
  "private": true,
  "scripts": {
    "serve-dev": "babel-node --require node_modules/dotenv/config --no-config --presets=@babel/env,@babel/typescript --extensions=.ts node_modules/.bin/micro-dev ./src/index.ts --watch ./src --ignore ./src/__generated__ --port 4000",
    "dev": "npm-run-all --parallel 'serve-dev' 'build-schema'",
    "build-schema": "NODE_ENV=development nodemon -i ./src/__generated__ -w ./src --ext .ts --exec babel-node --require node_modules/dotenv/config --no-config --presets=@babel/env,@babel/typescript --extensions=.ts ./buildSchema",
    "postdeploy": "prisma2 lift up && npm run seed",
    "postinstall": "prisma2 generate",
    "gqlgen": "graphql-codegen --config codegen.yml",
    "seed": "node -r dotenv/config prisma/seed"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/typescript": "1.5.0",
    "@graphql-codegen/typescript-resolvers": "1.5.0",
    "@types/bcrypt": "3.0.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/micro-cors": "^0.1.0",
    "@types/node": "12.7.2",
    "dotenv": "^8.0.0",
    "graphql-codegen-typescript-resolvers": "^0.18.2",
    "isomorphic-unfetch": "^3.0.0",
    "micro-dev": "^3.0.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "prisma2": "^2.0.0-alpha.115",
    "typescript": "3.5.3",
    "wait-on": "^3.3.0"
  },
  "dependencies": {
    "apollo-server-micro": "2.8.1",
    "aws-sdk": "^2.511.0",
    "bcrypt": "^3.0.6",
    "cookie": "^0.4.0",
    "graphql": "^14.4.2",
    "jsonwebtoken": "^8.5.1",
    "micro-cors": "^0.1.1"
  }
}
