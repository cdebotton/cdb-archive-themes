{
  "name": "cdb-graphql",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w ./src --ext .ts --exec babel-node --require node_modules/dotenv/config --no-config --presets=@babel/env,@babel/typescript --extensions=.ts ./src/index",
    "postdeploy": "prisma2 lift up && npm run seed",
    "postinstall": "prisma2 generate",
    "gqlgen": "graphql-codegen --config codegen.yml",
    "seed": "node -r dotenv/config prisma/seed"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/typescript": "1.5.0",
    "@graphql-codegen/typescript-resolvers": "1.5.0",
    "@types/bcrypt": "3.0.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/node": "12.7.2",
    "dotenv": "^8.0.0",
    "graphql-codegen-typescript-resolvers": "^0.18.2",
    "isomorphic-unfetch": "^3.0.0",
    "nodemon": "^1.19.1",
    "prisma2": "^2.0.0-alpha.115",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "apollo-server": "2.8.1",
    "bcrypt": "^3.0.6",
    "cookie": "^0.4.0",
    "graphql": "^14.4.2",
    "jsonwebtoken": "^8.5.1"
  }
}
