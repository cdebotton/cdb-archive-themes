generator photon {
  provider  = "photonjs"
  platforms = ["native", "linux-glibc-libssl1.0.1"]
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

model User {
  id        String    @default(cuid()) @unique @id
  email     String    @unique
  password  String
  firstName String?
  lastName  String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  lastLogin DateTime?
}